<Window x:Class="Comisión_Estatal_de_Búsqueda_del_Estado_de_Veracruz.dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Comisión_Estatal_de_Búsqueda_del_Estado_de_Veracruz"
        mc:Ignorable="d"
        Title="Dashboard"
        Background="Transparent"
        MinWidth="1024"
        MinHeight="768">
    <Border Background="#fff">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="75"/>
                <RowDefinition/>
            </Grid.RowDefinitions>


            <StackPanel Grid.Row="0" Background="#2f3338">
                <Image Width="180">
                    <Image.Source>
                        <BitmapImage UriSource="/media/images/logo_letras.png"/>
                    </Image.Source>
                </Image>
            </StackPanel>

            <StackPanel Grid.Row="1" Background="#2f3338">

            </StackPanel>

            <StackPanel Grid.Row="1" Grid.Column="1" Background="#e9eff5">
                
            </StackPanel>

        </Grid>
    </Border>
</Window>
